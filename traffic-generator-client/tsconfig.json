{
    "compilerOptions": {
        "target": "es5",
        "jsx": "react",
        "noEmitOnError": true,
        "noUnusedLocals": true,
        "removeComments": true,
        "noImplicitAny": true,
        "esModuleInterop": true,
        "baseUrl": "./",
        "paths": { 
            "app": ["src/app"],
            "pages/*": ["src/pages/*"],
        }
    },
    "exclude": [
        "node_modules"
    ]
}